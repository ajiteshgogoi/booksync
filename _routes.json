{
  "version": 1,
  "include": [
    "/*"
  ],
  "exclude": [
    "/api/*"
  ],
  "routes": [
    {
      "src": "/assets/.*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "\\.(js|css|ico|png|jpg|jpeg|svg|json)$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/[^.]*",
      "dest": "/index.html"
    }
  ]
}