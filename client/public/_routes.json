{
  "version": 1,
  "routes": [
    { 
      "src": "/api/.*",
      "dest": "/api/[route]" 
    },
    {
      "src": "^/assets/.*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": ".*",
      "dest": "/index.html",
      "status": 200
    }
  ]
}