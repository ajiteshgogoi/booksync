{
  "version": 1,
  "include": [
    "/*"
  ],
  "exclude": [
    "/api/*"
  ],
  "routes": [
    {
      "src": "^/assets/.+",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "^/[^.]+$",
      "dest": "/index.html",
      "status": 200
    }
  ],
  "platformErrorOverrides": [
    {
      "status": 404,
      "dest": "/index.html"
    }
  ]
}